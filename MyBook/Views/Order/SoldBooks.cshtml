@model SoldBooksViewModel
@{
    ViewBag.Title = "გაყიდული წიგნები";
}

@section HeaderSection {
    @Html.DevExpress().GetStyleSheets(new StyleSheet { ExtensionSuite = ExtensionSuite.GridView })
    @Html.DevExpress().GetScripts(new Script { ExtensionSuite = ExtensionSuite.GridView })
}

@section Breadcramps{
    <ol class="breadcrumb">
        <li class="active"><a href="#"><i class="fa fa-dashboard"></i> @ViewBag.Title</a></li>
    </ol>
}

<div class="row" id="sold-books-grid">
    <div class="col-md-12">
        <div class="box">
            <div class="box-header with-border">
                <div class="box-tools pull-right">

                </div>
            </div>
            <div class="box-body">
                @Html.Partial("_SoldBooksGrid", Model.GridViewModel)
            </div>
        </div>
    </div>
</div>


@section FooterSection {
    <script src="~/Content/scripts/Order/SoldBooks.js"></script>
}
